CREATE OR REPLACE FUNCTION "expenses"()
 RETURNS void
 LANGUAGE plpgsql
AS $function$
    BEGIN
    drop table IF EXISTS expenses;
    create table IF NOT EXISTS expenses
    (
        expenses_id SERIAL,
        merit_exp varchar(100),
        type_exp enum('salary' ,'tax/permissions','bill', 'rental','etc') ,
        primary key(expenses_id)
    );
    END;
$function$
